{
  "name": "@janvorisek/drie",
  "version": "1.0.0-alpha-1.48",
  "private": false,
  "description": "Drie is a Vue 3 component library for three.js made with TypeScript and Composition API",
  "keywords": [
    "vue3",
    "component",
    "three",
    "three.js",
    "3D"
  ],
  "homepage": "https://www.drie.dev",
  "repository": "github:janvorisek/drie",
  "license": "MIT",
  "author": "Jan Vorisek",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "vite build",
    "demo:build": "vite build --config demo/vite.config.ts",
    "demo:dev": "vite dev --config demo/vite.config.ts",
    "dev": "vite",
    "docgen": "del-cli docs/components && vue-docgen src/**/*.vue docs/_autogenerated && move-cli docs/_autogenerated/src/ docs/components && del-cli docs/_autogenerated && ts-node fix_docs",
    "docs:prepare": "cpy node_modules/shiki/languages docs/public/languages --flat && cpy node_modules/shiki/themes docs/public/themes --flat && cpy node_modules/vscode-oniguruma/release/onig.wasm docs/public --flat",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "fix": "eslint --fix \"**/*.{vue,ts,js}\"",
    "lint": "eslint \"**/*.{vue,ts,js}\" --fix",
    "release": "release-it *",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@mdit-vue/shared": "^0.12.0",
    "@tweakpane/plugin-essentials": "^0.1.5",
    "@types/node": "^18.11.17",
    "@types/three": "^0.150.1",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.6",
    "cpy-cli": "^4.2.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "flexsearch": "^0.7.31",
    "gh-pages": "5.0.0",
    "happy-dom": "^9.1.9",
    "husky": "^8.0.2",
    "markdown-it": "^13.0.1",
    "move-cli": "^2.0.0",
    "prettier": "^2.8.1",
    "release-it": "^15.6.0",
    "shiki": "^0.14.1",
    "sitemap": "^7.1.1",
    "three": "github:mrdoob/three.js#dev",
    "tweakpane": "^3.1.2",
    "typescript": "^5.0.4",
    "v-tweakpane": "^0.2.0",
    "vite": "^4.0.3",
    "vite-plugin-dts": "2.2.0",
    "vitepress": "^1.0.0-alpha.33",
    "vitest": "^0.31.1",
    "vue": "^3.2.45",
    "vue-docgen-cli": "^4.56.1",
    "vue-tsc": "^1.0.18"
  },
  "peerDependencies": {
    "three": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "ts-event-bus": "^4.1.1"
  }
}
