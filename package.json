{
  "name": "@janvorisek/drie",
  "version": "1.0.0-alpha-1.9",
  "description": "Drie is a Vue 3 component library for THREE.js made with TypeScript and Composition API",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "demo:build": "vite build --config demo/vite.config.ts",
    "demo:dev": "vite dev --config demo/vite.config.ts",
    "lint": "eslint \"**/*.{vue,ts,js}\" --fix",
    "fix": "eslint --fix \"**/*.{vue,ts,js}\"",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "docgen": "del-cli docs/components && vue-docgen src/**/*.vue docs/_autogenerated && move-cli docs/_autogenerated/src/ docs/components && del-cli docs/_autogenerated && ts-node fix_docs",
    "docs:dev": "npm run docgen && vitepress dev docs",
    "docs:build": "npm run docgen && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "release": "release-it *"
  },
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "vue3",
    "component",
    "vite",
    "THREE"
  ],
  "author": "Jan Vorisek",
  "license": "MIT",
  "homepage": "https://www.drie.dev",
  "repository": "github:janvorisek/drie",
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/three": "^0.147.1",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.6",
    "bumpp": "8.2.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "gh-pages": "4.0.0",
    "happy-dom": "^8.1.1",
    "husky": "^8.0.2",
    "move-cli": "^2.0.0",
    "prettier": "^2.8.1",
    "release-it": "^15.6.0",
    "rimraf": "3.0.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-dts": "1.7.1",
    "vitepress": "^1.0.0-alpha.33",
    "vitest": "^0.26.2",
    "vue": "^3.2.45",
    "vue-docgen-cli": "^4.56.1",
    "vue-tsc": "^1.0.18"
  },
  "dependencies": {
    "three": "github:mrdoob/three.js#dev"
  }
}
